{
	"name": "ft_trancendence",
	"version": "1.0.0",
	"description": "ft_trancendence project",
	"main": "index.js",
	"scripts": {
		"dev": "tsx backend/src/index.ts",
		"backend:dev": "cd backend && npm run dev",
		"build": "tsc",
		"lint": "eslint --config config/.eslintrc.js 'src/**/*.ts' --no-error-on-unmatched-pattern",
		"lint:fix": "eslint --config config/.eslintrc.js 'src/**/*.ts' --fix --no-error-on-unmatched-pattern",
		"format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
		"type-check": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"test": "jest --config config/jest.config.mjs",
		"test:coverage": "jest --config config/jest.config.mjs --coverage",
		"test:watch": "jest --config config/jest.config.mjs --watch",
		"pr-comments": "./pr_comments_to_md.sh"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@evilmartians/lefthook": "^1.13.3",
		"@playwright/test": "^1.56.1",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"eslint": "^8.57.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.6.2",
		"ts-jest": "^29.1.2",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3"
	},
	"dependencies": {
